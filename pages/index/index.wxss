/**index.wxss**/
.container{
  position: absolute;
  left:0;
  right:0;
  top:0;
  bottom: 80rpx;
}
.container map{
  width:100%;
  height:100%;
}
.container .foot{
  display: flex;
  width:100%;
  height:80rpx;

}
.container .foot view{
  flex: 1;
  text-align: center;
  line-height: 80rpx;
  background: #03BBD5;
  color:#fff;

}
.container .foot view:nth-child(1){
  background: #ff9700;
}